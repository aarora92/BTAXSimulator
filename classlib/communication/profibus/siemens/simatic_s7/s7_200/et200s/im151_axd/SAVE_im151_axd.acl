<![CDATA[PROGRAM horst
	VAR_INPUT
		xxx : BOOL;
	END_VAR	
	VAR
		res : REAL := 23.4;
		a : SINT;
		x : WORD;
		i : INT;
		value0,value1 : INT;
		init : BOOL := FALSE;
		index : USINT;
		err : USINT;
		(*stra,strb :STRING;*)
	END_VAR
	VAR_EXTERNAL
		in0,in1 : BOOL;
		out0,out1 : BOOL;
		bOut0 : BYTE;
