		M03_CHI0,M03_CHI1  : WORD;
		M11_CHQ0 : WORD;
		error : ARRAY [0..63] OF USINT; 
		outval : WORD;
	END_VAR
	a := a + 1;			
	bOut0 := a;
	x := x + 50;	
	M06_CHQ0 := x;
	M06_CHQ1 := x;
	value0 := M03_CHI0;
	value1 := M03_CHI1;
	IF NOT init THEN
		FOR i := 0 TO 63 DO
		   error[i] := 0; 
		END_FOR;
		init := TRUE;
		M11_CHQ0 := 27000;
		
	END_IF;	
	err := error[index];	
END_PROGRAM
