		 SBUS_slot	   : USINT;	
		 SYS_stop : BOOL;
		 error : ARRAY [0..63] OF USINT; 
		 M11_CHQ0 :WORD;
		 
	END_VAR
	
	
	VAR
		test : USINT;
		cnt : INT;
	END_VAR
	SBUS_any := FALSE;
	SBUS_plugged := FALSE;
	SBUS_pulled := FALSE;
	SBUS_wrongmod := FALSE; 
	IF SBUS_diag THEN
		SBUS_diag := FALSE;
		IF SBUS_DiagSlot = 9 THEN
			IF SBUS_DiagType THEN
				M11_CHQ0 := 100;
			ELSE
				M11_CHQ0 := 27000;
			END_IF;	
		END_IF; 
		cnt := cnt + 1;
		error[SBUS_DiagSlot] := SBUS_DiagType;
	END_IF;	
	(*SYS_stop := TRUE;*)
	IF SBUS_slot = 3 THEN
		SBUS_slot := 16#FF;
		SYS_stop := TRUE;
	END_IF;	
END_PROGRAM
